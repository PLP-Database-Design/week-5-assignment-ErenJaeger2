<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Providers Data</title>
  </head>
  <body>
    <h1>List of Providers</h1>
    <table border="1">
      <thead>
        <tr>
          <th>Provider ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Specialty</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach(provider => { %>
        <tr>
          <td><%= provider.provider_id %></td>
          <td><%= provider.first_name %></td>
          <td><%= provider.last_name %></td>
          <td><%= provider.provider_specialty %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
